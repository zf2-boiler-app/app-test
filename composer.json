{
    "name": "zf2-boiler-app/app-test",
    "license": "MIT",
    "type": "library",
    "description": "ZF2 BoilerApp tests tools",
    "keywords": ["Zend Framework 2","ZF2 Boiler-App","tests","PHPUnit","Doctrine"],
    "authors": [
        {
            "name": "Neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zf2-boiler-app/app-test/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-master",
        "phpunit/phpunit": "*@stable",
        "doctrine/orm": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "neilime/zf2-deploy-module": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "BoilerAppTest": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
